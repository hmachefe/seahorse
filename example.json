{
  "routes": [{
    "httpRequest" : {
        "method" : "post",
        "path" : "/help"
    },
    "httpResponse" : {
        "statusCode" : 200,
        "body" : "{\"key\": \"value\"}",
        "headers" : [ {
            "name": "Access-Control-Allow-Headers",
            "values": ["Content-Type, Authorization"]
        },
        {
            "name": "Content-Type",
            "values": ["application/json; charset=utf-8"]
        } ],
        "delay": {
            "value": 1
        }
    }
  },
  {
    "httpRequest" : {
        "method" : "get",
        "path" : "/help",
        "queryString" : "id=1"
    },
    "httpResponse" : {
        "statusCode" : 200,
        "file":"/lib/help.txt",
        "headers" : [ {
            "name": "Content-Type",
            "values": ["application/json"]
        } ],
        "delay": {
            "value": 1000
        }
    }
  }]
}
